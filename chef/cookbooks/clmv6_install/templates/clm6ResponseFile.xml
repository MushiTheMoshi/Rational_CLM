    <data key='user.com.ibm.team.install.jfs.sso.oidc.enable' value='false'/>
    <data key='cic.selector.nl' value='en'/>
  </profile>
  <install modify='false'>
    <!-- Change and Configuration Management 6.0 -->
    <offering profile='IBM Collaborative Lifecycle Management' id='com.ibm.team.install.jfs.app.rtc' version='6.0.0.RTC-I20150519-1214-r60' features='jfs.base,db.derby,license,swg.tags,itlm,repotools,scripts,upgrade,server.conf,server.provision,tomcat.webapps,liberty.webapps,noappserver.webapps' installFixes='none'/>
    <!-- Jazz Team Server 6.0 -->
    <offering profile='IBM Collaborative Lifecycle Management' id='com.ibm.team.install.jfs.app.jts' version='6.0.0.RJF-I20150519-1056-r60-' features='jfs.base,jazz,clmhelp,rcl' installFixes='none'/>
    <!-- Quality Management 6.0 -->
    <offering profile='IBM Collaborative Lifecycle Management' id='com.ibm.team.install.jfs.app.rqm' version='6.0.0.RQM6_0_0-I20150519_1442' features='jfs.base,db.derby,license,repotools,scripts,upgrade,server.conf,server.provision,tomcat.webapps,liberty.webapps,noappserver.webapps,swg.tags,itlm' installFixes='none'/>
    <!-- Requirements Management 6.0 -->
    <offering profile='IBM Collaborative Lifecycle Management' id='com.ibm.team.install.jfs.app.rdm' version='6.0.0.RDNG6_0_0-I20150519_1236' features='jfs.base,db.derby,license,upgrade,server.conf,server.provision,tomcat.webapps,liberty.webapps,noappserver.webapps,swg.tags,itlm,repotools,scripts' installFixes='none'/>
    <!-- Trial keys for Collaborative Lifecycle Management Products 6.0 -->
    <offering profile='IBM Collaborative Lifecycle Management' id='com.ibm.team.install.jfs.app.product-clm' version='6.0.0.CALM60-I20150519-1808-r60' features='server.conf,server.provision,require.jts' installFixes='none'/>
  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='${sharedLocation}'/>
  <preference name='com.ibm.cic.common.core.preferences.connectTimeout' value='30'/>
  <preference name='com.ibm.cic.common.core.preferences.readTimeout' value='45'/>
  <preference name='com.ibm.cic.common.core.preferences.downloadAutoRetryCount' value='0'/>
  <preference name='offering.service.repositories.areUsed' value='true'/>
  <preference name='com.ibm.cic.common.core.preferences.ssl.nonsecureMode' value='false'/>
  <preference name='com.ibm.cic.common.core.preferences.http.disablePreemptiveAuthentication' value='false'/>
